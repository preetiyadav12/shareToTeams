{
  "name": "@msteams/sharetoteam",
  "version": "0.0.4",
  "description": "Microsoft Teams share To Teams control",
  "author": "Microsoft",
  "license": "MIT",
  "main": "dist/shareToTeams.js",
  "types": "dist/types/index.d.ts",
  "keywords": [
    "framework",
    "typescript",
    "scss",
    "npm"
  ],
  "files": [
    "dist"
  ],
  "scripts": {
    "clean": "rimraf dist",
    "prebuild": "npm run clean",
    "unpublish:local": "yarn unlink",
    "publish:local": "yarn link",
    "npm:local": "run-s unpublish:local publish:local",
    "prepare": "run-s build:dev publish:local",
    "prepublishOnly": "yarn test && yarn lint",
    "preversion": "yarn lint",
    "build:dev": "tsc --build ./tsconfig.build.json && webpack --config ./webpack.dev.config.ts",
    "build:deploy": "tsc --build ./tsconfig.build.json && webpack --config ./webpack.prod.config.ts",
    "build": "run-s build:dev npm:local upload",
    "build:prod": "run-s build:deploy npm:local upload",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "version": "yarn format && git add .",
    "postversion": "git push && git push --tags",
    "test": "jest --config jestconfig.json",
    "upload": "node uploadpkg.js"
  },
  "devDependencies": {
    "@azure/communication-calling": "1.4.4",
    "@azure/communication-chat": "1.1.0",
    "@azure/communication-common": "1.1.0",
    "@azure/msal-browser": "^2.24.0",
    "@azure/storage-blob": "^12.9.0",
    "@babel/core": "^7.17.10",
    "@babel/plugin-transform-runtime": "^7.17.10",
    "@babel/preset-env": "^7.17.10",
    "@babel/preset-react": "^7.16.7",
    "@babel/preset-typescript": "^7.16.7",
    "@babel/runtime": "^7.17.9",
    "@types/jest": "^27.5.0",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.23.0",
    "@typescript-eslint/parser": "^5.23.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^8.3.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "copy-webpack-plugin": "^10.2.4",
    "css-loader": "^5.2.4",
    "dotenv": "^16.0.1",
    "eslint": "^8.15.0",
    "eslint-plugin-no-loops": "^0.3.0",
    "eslint-webpack-plugin": "^3.1.1",
    "fork-ts-checker-webpack-plugin": "^7.2.11",
    "html-webpack-plugin": "^5.5.0",
    "jest": "^28.1.0",
    "jest-environment-jsdom": "^28.1.3",
    "jest-environment-node": "^28.1.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.6.2",
    "sass": "^1.32.13",
    "sass-loader": "^11.1.1",
    "source-map-loader": "^3.0.1",
    "style-loader": "^2.0.0",
    "ts-jest": "^28.0.2",
    "ts-loader": "^9.1.2",
    "ts-node": "^10.7.0",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.2.4",
    "uuid": "^8.3.2",
    "webpack": "^5.72.0",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.8.1"
  },
  "dependencies": {
    "@microsoft/mgt": "^2.7.1",
    "@microsoft/mgt-teamsfx-provider": "^2.7.1",
    "@microsoft/teamsfx": "^2.0.0",
    "adaptivecards": "^3.0.0-beta.9",
    "adaptivecards-templating": "^2.3.1"
  }
}
